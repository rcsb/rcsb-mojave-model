{
  "type": "object",
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "title" : "core_entity.json",
  "required":[
    "rcsb_entity_container_identifiers"
  ],
  "properties": {
    "rcsb_entity_container_identifiers": {
      "type": "object",
      "properties": {
        "uniprot_ids": {
          "type": "array",
          "minItems": 1,
          "uniqueItems": true,
          "items": {
            "type": "string"
          }
        },
        "go_ids": {
          "type": "array",
          "minItems": 1,
          "uniqueItems": true,
          "items": {
            "type": "string"
          }
        },
        "pfam_ids": {
          "type": "array",
          "minItems": 1,
          "uniqueItems": true,
          "items": {
            "type": "string"
          }
        }
      }
    },
    "rcsb_membrane_reference": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "Identifier for membrane classification term."
        },
        "name": {
          "type": "string",
          "description": "Membrane protein classification term."
        },
        "provenance_code": {
          "type": "string",
          "enum": [
            "MpStruct",
            "Homology"
          ],
          "description": ""
        }
      },
      "description": "Information about integral membrane proteins whose crystallographic, or sometimes NMR or cryo-EM, structures \nhave been determined to a resolution sufficient to identify TM helices of helix-bundle membrane proteins (typically 4 - 4.5 A)."
    },
    "rcsb_membrane_lineage": {
      "type" : "array",
      "minItems" : 1,
      "uniqueItems" : true,
      "items" : {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": ""
          },
          "name":{
            "type": "string",
            "description": ""
          }
        },
        "description" : "An array the ID(s) listing the reference lineage."
      }
    },
    "rcsb_enzyme_reference": {
      "type" : "array",
      "minItems" : 1,
      "uniqueItems" : true,
      "items" : {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": ""
          },
          "name": {
            "type": "string",
            "description": ""
          },
          "provenance_code": {
            "type": "string",
            "enum": [
              "PDB primary data"
            ],
            "description": ""
          }
        },
        "description": ""
      }
    },
    "rcsb_enzyme_lineage": {
      "type" : "array",
      "minItems" : 1,
      "uniqueItems" : true,
      "items" : {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": ""
          },
          "name":{
            "type": "string",
            "description": ""
          }
        },
        "description" : "An array the ID(s) listing the reference lineage."
      }
    },
    "rcsb_go_reference": {
      "type" : "array",
      "minItems" : 1,
      "uniqueItems" : true,
      "items" : {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "examples": ["GO:0005125", "GO:0060092"],
            "description": "A unique seven digit identifier prefixed by GO:."
          },
          "name": {
            "type": "string",
            "examples": [ "response to muscle activity", "sulfur utilization", "cell killing"],
            "description": "A human-readable term name."
          }
        }
      },
      "description": "Ontologies that describe gene products in terms of their associated \nbiological processes, cellular components and molecular functions in a species-independent manner."
    },
    "rcsb_go_lineage": {
      "type" : "array",
      "minItems" : 1,
      "uniqueItems" : true,
      "items" : {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "examples": ["GO:0005125", "GO:0060092"],
            "description": "A unique seven digit identifier prefixed by GO:."
          },
          "name":{
            "type": "string",
            "examples": [ "response to muscle activity", "sulfur utilization", "cell killing"],
            "description": "A human-readable term name."
          }
        }
      }
    },
    "rcsb_gene_info": {
      "type":"array",
      "items" : {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "The official gene name."
          },
          "type": {
            "enum": [
              "PRIMARY",
              "SYNONYM"
            ],
            "type": "string"
          },
          "approved": {
            "type": "string",
            "enum": [
              "Y",
              "N"
            ],
            "description": "Indicates if gene name is an HGNC-approved gene nomenclature."
          },
          "provenance_code": {
            "type": "string",
            "enum": [
              "UniProt"
            ]
          }
        }
      }
    },
    "rcsb_cluster_flexibility" : {
      "type" : "object",
      "properties" : {
        "link" : {
          "type" : "string",
          "description" : "Link to the associated PDBFlex database entry."
        },
        "label" : {
          "type" : "string",
          "description" : "Structural flexibility in the cluster (95% sequence identity) where a given entity belongs."
        },
        "avg_rmsd" : {
          "type" : "number",
          "description" : "Average RMSD refer to average pairwise RMSD (Root Mean Square Deviation of C-alpha atoms)\n between structures in the cluster (95% sequence identity) where a given entity belongs."
        },
        "max_rmsd" : {
          "type" : "number",
          "description" : "Maximal RMSD refer to maximal pairwise RMSD (Root Mean Square Deviation of C-alpha atoms)\n between structures in the cluster (95% sequence identity) where a given entity belongs."
        },
        "provenance_code": {
          "type": "string",
          "enum": [
            "PDBFlex"
          ]
        }
      },
      "description": "Indicates intrinsic flexibility of protein structures determined from structural variations\n between different depositions and chains in asymmetric units of the same protein in PDB (95% sequence identity)."
    }
  },
  "additionalProperties": false
}
