{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "rcsb_id",
    "rcsb_group_container_identifiers",
    "rcsb_group_info"
  ],
  "properties": {
    "rcsb_id": {
      "type": "string",
      "description": "A unique textual identifier for a group"
    },
    "rcsb_group_container_identifiers": {
      "type": "object",
      "required": [
        "group_id",
        "group_provenance_id",
        "group_member_ids"
      ],
      "properties": {
        "group_id": {
          "type": "string",
          "description": "A unique textual identifier for a group"
        },
        "group_provenance_id": {
          "type": "string",
          "description": "This data item is a pointer to a group provenance definition"
        },
        "parent_member_ids": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "uniqueItems": true,
          "description": "Member identifiers representing a higher level in the groping hierarchy that has parent-child relationship"
        },
        "group_member_ids": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "uniqueItems": true,
          "description": "Member identifiers representing a group"
        }
      },
      "additionalProperties": false
    },
    "rcsb_group_info": {
      "type": "object",
      "required": [
        "group_name",
        "group_description",
        "group_members_granularity",
        "group_members_count"
      ],
      "properties": {
        "group_name": {
          "type": "string"
        },
        "group_description": {
          "type": "string"
        },
        "group_members_granularity": {
          "type": "string",
          "enum": [
            "assembly",
            "polymer_entity",
            "polymer_entity_instance"
          ],
          "description": "Granularity of group members identifiers"
        },
        "group_members_count": {
          "type": "integer"
        }
      }
    },
    "rcsb_group_statistics": {
      "type": "object",
      "properties": {
        "similarity_score_min": {
          "type": "number",
          "description": "Similarity score between two least similar group members"
        },
        "similarity_score_max": {
          "type": "number",
          "description": "Similarity score between two most similar group members"
        }
      },
      "additionalProperties": false
    },
    "rcsb_group_accession_info": {
      "type": "object",
      "required": [
        "version"
      ],
      "properties": {
        "version": {
          "type": "integer",
          "description": "Identifies the version of the groups solution"
        }
      },
      "additionalProperties": false
    },
    "rcsb_group_related": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "resource_accession_code": {
            "type": "string",
            "examples": [
              "P69905"
            ],
            "description": "A unique code assigned to a reference related the group"
          },
          "resource_name": {
            "type": "string",
            "examples": [
              "UniProt"
            ],
            "description": "Defines the type of the resource describing related references"
          }
        }
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "rcsb_group_members_rankings": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "representative_criteria_type",
          "ranking_options"
        ],
        "properties": {
          "representative_criteria_type": {
            "type": "string",
            "enum": [
              "best coverage",
              "best resolution"
            ],
            "rcsb_enum_annotated": [
              {
                "value": "best resolution",
                "detail": "The lower the resolution value, the better"
              },{
                "value": "best coverage",
                "detail": "The higher the coverage value, the better"
              }
            ],
            "description": "Uniquely identifies the criteria incorporated into selecting a representative group member"
          },
          "ranking_options": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "ranking_criteria_type",
                "group_members"
              ],
              "properties": {
                "ranking_criteria_type": {
                  "type": "string",
                  "enum": [
                    "RMSD",
                    "data quality",
                    "number of modified residues"
                  ],
                  "rcsb_enum_annotated": [
                    {
                      "value": "RMSD",
                      "detail": "The lower the RMSD, the better the structural similarity"
                    }
                  ],
                  "description": "Defines ranking option applicable to group members"
                },
                "group_members": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "required": [
                      "member_id",
                      "rank"
                    ],
                    "properties": {
                      "member_id": {
                        "type": "string"
                      },
                      "rank": {
                        "type": "integer",
                        "description": "Reflects a relationship between group members such that, for any two members the first is ranked higher (smaller rank value) than the second"
                      },
                      "original_score": {
                        "type": "number",
                        "description": "Quantifies the criteria used for ranking"
                      }
                    },
                    "additionalProperties": false
                  },
                  "minItems": 1,
                  "uniqueItems": true
                }
              },
              "additionalProperties": false
            },
            "minItems": 1,
            "uniqueItems": true
          }
        },
        "additionalProperties": false
      },
      "minItems": 1,
      "uniqueItems": true
    }
  },
  "additionalProperties": false
}
