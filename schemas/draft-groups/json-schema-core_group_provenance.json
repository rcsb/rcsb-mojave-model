{
  "type": "object",
  "properties": {
    "rcsb_id": {
      "type": "string",
      "description": "A unique group provenance identifier"
    },
    "rcsb_group_provenance_container_identifiers": {
      "type": "object",
      "required": [
        "group_provenance_id"
      ],
      "properties": {
        "group_provenance_id": {
          "type": {
            "type": "string",
            "description": "A unique group provenance identifier"
          }
        }
      }
    },
    "rcsb_group_aggregation_method": {
      "type": "object",
      "required": [
        "type",
        "method"
      ],
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "structure similarity",
            "sequence similarity",
            "reference identity"
          ],
          "description": "Specifies the type of similarity criteria used to aggregate members into higher levels in the hierarchy"
        },
        "method": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "name"
            ],
            "properties": {
              "name": {
                "type": "string",
                "enum": [
                  "mmseqs2",
                  "matching UniProt accession",
                  "matching UniProt accession list"
                ],
                "examples": [
                  "mmseqs2"
                ],
                "description": "The name of the software or the method used to calculate cluster solutions"
              },
              "version": {
                "type": "string",
                "examples": [
                  "v1.0",
                  "3.1-2",
                  "unknown"
                ],
                "description": "The version of the software."
              },
              "similarity_criteria": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "similarity_function": {
                      "type": "string",
                      "enum": [
                        "RMSD",
                        "sequence identity"
                      ],
                      "examples": [
                        "RMSD"
                      ],
                      "description": "A function or similarity measure that quantifies the similarity between two members",
                      "rcsb_enum_annotated": [
                        {
                          "value": "RMSD",
                          "detail": "The lower the RMSD, the higher the similarity between two group members",
                          "units": "Ã…"
                        },
                        {
                          "value": "sequence identity",
                          "detail": "The higher the sequence identity, the higher the similarity between two group members",
                          "units": "%"
                        }
                      ]
                    },
                    "similarity_threshold": {
                      "type": "number",
                      "description": "The desired lower limit for the similarity between two members that belong to the same group"
                    }
                  }
                }
              },
              "details": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string",
                      "description": "Defines the type of the description associated with the clustering process"
                    },
                    "name": {
                      "type": "string",
                      "description": "Defines the name of the description associated with the clustering process"
                    },
                    "value": {
                      "type": "number",
                      "description": "Defines the value associated with the clustering process"
                    },
                    "description": {
                      "type": "string",
                      "description": "A description of special aspects of the clustering process"
                    }
                  }
                },
                "description": "Additional details describing the clustering process"
              }
            },
            "additionalProperties": false
          },
          "description": "The details on a method used to calculate cluster solutions"
        }
      },
      "additionalProperties": false
    },
    "rcsb_group_representative_criteria": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "representative_criteria_id": {
            "type": "string",
            "description": "Uniquely identifies the criteria incorporated into selecting a representative group member"
          },
          "description": {
            "type": "string",
            "description": "Describes the criteria incorporated into selecting a representative group member"
          }
        }
      }
    },
    "rcsb_group_ranking_criteria": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "ranking_criteria_type"
        ],
        "properties": {
          "ranking_criteria_type": {
            "type": "string",
            "enum": [
              "RMSD"
            ],
            "rcsb_enum_annotated": [
              {
                "value": "RMSD",
                "detail": ""
              }
            ]
          }
        }
      },
      "description": "Defines ranking options applicable to group members"
    }
  },
  "additionalProperties": false
}
