{
  "title" : "Entry Group",
  "$schema": "https://json-schema.org/draft-07/schema#",
  "$id": "/groups/json-schema-group_entry.json",
  "type": "object",
  "properties": {
    "rcsb_entry_group_members_rankings": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "ranking_criteria_type",
          "group_members"
        ],
        "properties": {
          "ranking_criteria_type": {
            "type": "string",
            "enum": [
              "member_quality"
            ],
            "rcsb_enum_annotated": [
              {
                "value": "member_quality",
                "detail": "Complex quality metric. The higher, the better"
              }
            ],
            "description": "Defines ranking option applicable to group members",
            "rcsb_search_context": [
              "exact-match"
            ]
          },
          "group_members": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "member_id",
                "rank"
              ],
              "properties": {
                "member_id": {
                  "type": "string",
                  "rcsb_search_context": [
                    "exact-match"
                  ]
                },
                "rank": {
                  "type": "integer",
                  "description": "Reflects a relationship between group members such that, for any two members the first is ranked higher (smaller rank value) than the second",
                  "rcsb_search_context": [
                    "default-match"
                  ]
                },
                "original_score": {
                  "type": "number",
                  "description": "Quantifies the criteria used for ranking",
                  "rcsb_search_context": [
                    "default-match"
                  ]
                }
              },
              "additionalProperties": false
            },
            "minItems": 1,
            "uniqueItems": true
          }
        },
        "additionalProperties": false
      },
      "minItems": 1,
      "uniqueItems": true
    }
  }
}