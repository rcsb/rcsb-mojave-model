{
  "title" : "Structure Embedding",
  "description": "JSON schema for storing embedding data.",
  "type": "object",
  "required": [
    "rcsb_embedding_info",
    "rcsb_id",
    "embedding"
  ],
  "properties": {
    "rcsb_id": {
      "type": "string",
      "examples": [
        "1KIP"
      ],
      "description": "A unique identifier for each object in this entry container."
    },
    "rcsb_embedding_info": {
         "type": "object",
         "properties": {
           "entry_id": {
             "type": "string",
             "examples": [
               "4HHB",
               "AF_AFP60325F1",
               "MA_MABAKCEPC0019"
             ],
             "description": "Entry identifier for the container."
           },
           "major_revision": {
               "type": "integer",
               "description": "The latest entry major revision number."
            },
            "minor_revision": {
               "type": "integer",
               "description": "The latest entry minor revision number."
            }
         },
         "additionalProperties": false
    },
    "embedding": {
      "type": "array",
      "minItems": 1536,
      "maxItems": 1536,
      "items": {
         "type": "number",
         "examples": [
           108.11,
           3.14,
           2.71
         ],
         "description": "Fixed length vector encoding a 3D macromolecular structure."
      },
      "uniqueItems": false
    },
    "n_residues": {
      "type": "integer",
      "description": "Number of encoded residues in the embedding"
    }
  }
}