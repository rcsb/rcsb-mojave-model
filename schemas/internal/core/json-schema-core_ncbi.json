{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "rcsb_ncbi_containers_identifiers",
    "rcsb_ncbi_metada",
    "rcsb_ncbi_chromosome",
    "rcsb_ncbi_coordinates",
    "rcsb_ncbi_organism"
  ],
  "properties": {
    "rcsb_ncbi_containers_identifiers": {
      "$id": "#/properties/rcsb_ncbi_containers_identifiers",
      "type": "object",
      "title": "The Rcsb_ncbi_containers_identifiers Schema",
      "required": [
        "gene_id",
        "rna_nucleotide_accession",
        "rna_nucleotide_version",
        "rna_nucleotide_gi",
        "protein_accession",
        "protein_version",
        "protein_gi",
        "symbol",
        "uniprot_accession"
      ],
      "properties": {
        "gene_id": {
          "$id": "#/properties/rcsb_ncbi_containers_identifiers/properties/gene_id",
          "type": "integer",
          "title": "The Gene_id Schema",
          "examples": [
            3265
          ]
        },
        "rna_nucleotide_accession": {
          "$id": "#/properties/rcsb_ncbi_containers_identifiers/properties/rna_nucleotide_accession",
          "type": "string",
          "title": "The Rna_nucleotide_accession Schema",
          "examples": [
            "NM_005343"
          ],
          "pattern": "^(.*)$"
        },
        "rna_nucleotide_version": {
          "$id": "#/properties/rcsb_ncbi_containers_identifiers/properties/rna_nucleotide_version",
          "type": "integer",
          "title": "The Rna_nucleotide_version Schema",
          "examples": [
            4
          ]
        },
        "rna_nucleotide_gi": {
          "$id": "#/properties/rcsb_ncbi_containers_identifiers/properties/rna_nucleotide_gi",
          "type": "integer",
          "title": "The Rna_nucleotide_gi Schema",
          "examples": [
            568815587
          ]
        },
        "protein_accession": {
          "$id": "#/properties/rcsb_ncbi_containers_identifiers/properties/protein_accession",
          "type": "string",
          "title": "The Protein_accession Schema",
          "examples": [
            "NP_005334"
          ],
          "pattern": "^(.*)$"
        },
        "protein_version": {
          "$id": "#/properties/rcsb_ncbi_containers_identifiers/properties/protein_version",
          "type": "integer",
          "title": "The Protein_version Schema",
          "examples": [
            1
          ]
        },
        "protein_gi": {
          "$id": "#/properties/rcsb_ncbi_containers_identifiers/properties/protein_gi",
          "type": "integer",
          "title": "The Protein_gi Schema",
          "examples": [
            4885425
          ]
        },
        "symbol": {
          "$id": "#/properties/rcsb_ncbi_containers_identifiers/properties/symbol",
          "type": "string",
          "title": "The Symbol Schema",
          "examples": [
            "HRAS"
          ],
          "pattern": "^(.*)$"
        },
        "uniprot_accession": {
          "$id": "#/properties/rcsb_ncbi_containers_identifiers/properties/uniprot_accession",
          "type": "array",
          "title": "The Uniprot_accession Schema",
          "items": {
            "$id": "#/properties/rcsb_ncbi_containers_identifiers/properties/uniprot_accession/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "accession_id",
              "isoform_id"
            ],
            "properties": {
              "accession_id": {
                "$id": "#/properties/rcsb_ncbi_containers_identifiers/properties/uniprot_accession/items/properties/accession_id",
                "type": "string",
                "title": "The Accession_id Schema",
                "examples": [
                  "P01112"
                ],
                "pattern": "^(.*)$"
              },
              "isoform_id": {
                "$id": "#/properties/rcsb_ncbi_containers_identifiers/properties/uniprot_accession/items/properties/isoform_id",
                "type": "integer",
                "title": "The Isoform_id Schema",
                "examples": [
                  1
                ]
              }
            }
          }
        }
      }
    },
    "rcsb_ncbi_metada": {
      "$id": "#/properties/rcsb_ncbi_metada",
      "type": "object",
      "title": "The Rcsb_ncbi_metada Schema",
      "required": [
        "status"
      ],
      "properties": {
        "status": {
          "$id": "#/properties/rcsb_ncbi_metada/properties/status",
          "type": "string",
          "title": "The Status Schema",
          "examples": [
            "REVIEWED"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "rcsb_ncbi_chromosome": {
      "$id": "#/properties/rcsb_ncbi_chromosome",
      "type": "object",
      "title": "The Rcsb_ncbi_chromosome Schema",
      "required": [
        "genomic_nucleotide_accession",
        "genomic_nucleotide_version",
        "genomic_nucleotide_gi",
        "genomic_nucleotide_title"
      ],
      "properties": {
        "genomic_nucleotide_accession": {
          "$id": "#/properties/rcsb_ncbi_chromosome/properties/genomic_nucleotide_accession",
          "type": "string",
          "title": "The Genomic_nucleotide_accession Schema",
          "examples": [
            "NC_000011"
          ],
          "pattern": "^(.*)$"
        },
        "genomic_nucleotide_version": {
          "$id": "#/properties/rcsb_ncbi_chromosome/properties/genomic_nucleotide_version",
          "type": "integer",
          "title": "The Genomic_nucleotide_version Schema",
          "examples": [
            10
          ]
        },
        "genomic_nucleotide_gi": {
          "$id": "#/properties/rcsb_ncbi_chromosome/properties/genomic_nucleotide_gi",
          "type": "integer",
          "title": "The Genomic_nucleotide_gi Schema",
          "examples": [
            568815587
          ]
        },
        "genomic_nucleotide_title": {
          "$id": "#/properties/rcsb_ncbi_chromosome/properties/genomic_nucleotide_title",
          "type": "string",
          "title": "The Genomic_nucleotide_title Schema",
          "examples": [
            "Homo sapiens chromosome 11, GRCh38.p12 Primary Assembly"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "rcsb_ncbi_coordinates": {
      "$id": "#/properties/rcsb_ncbi_coordinates",
      "type": "object",
      "title": "The Rcsb_ncbi_coordinates Schema",
      "required": [
        "strat_position_on_the_genomic_accession",
        "end_position_on_the_genomic_accession",
        "orientation",
        "rna_coordinates",
        "protein_coordinates"
      ],
      "properties": {
        "strat_position_on_the_genomic_accession": {
          "$id": "#/properties/rcsb_ncbi_coordinates/properties/strat_position_on_the_genomic_accession",
          "type": "integer",
          "title": "The Strat_position_on_the_genomic_accession Schema",
          "examples": [
            532241
          ]
        },
        "end_position_on_the_genomic_accession": {
          "$id": "#/properties/rcsb_ncbi_coordinates/properties/end_position_on_the_genomic_accession",
          "type": "integer",
          "title": "The End_position_on_the_genomic_accession Schema",
          "examples": [
            535566
          ]
        },
        "orientation": {
          "$id": "#/properties/rcsb_ncbi_coordinates/properties/orientation",
          "type": "integer",
          "title": "The Orientation Schema",
          "examples": [
            2
          ]
        },
        "rna_coordinates": {
          "$id": "#/properties/rcsb_ncbi_coordinates/properties/rna_coordinates",
          "type": "array",
          "title": "The Rna_coordinates Schema",
          "items": {
            "$id": "#/properties/rcsb_ncbi_coordinates/properties/rna_coordinates/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "start_position_on_the_gene_sequence",
              "end_position_on_the_gene_sequence"
            ],
            "properties": {
              "start_position_on_the_gene_sequence": {
                "$id": "#/properties/rcsb_ncbi_coordinates/properties/rna_coordinates/items/properties/start_position_on_the_gene_sequence",
                "type": "integer",
                "title": "The Start_position_on_the_gene_sequence Schema",
                "examples": [
                  1
                ]
              },
              "end_position_on_the_gene_sequence": {
                "$id": "#/properties/rcsb_ncbi_coordinates/properties/rna_coordinates/items/properties/end_position_on_the_gene_sequence",
                "type": "integer",
                "title": "The End_position_on_the_gene_sequence Schema",
                "examples": [
                  152
                ]
              }
            }
          }
        },
        "protein_coordinates": {
          "$id": "#/properties/rcsb_ncbi_coordinates/properties/protein_coordinates",
          "type": "array",
          "title": "The Protein_coordinates Schema",
          "items": {
            "$id": "#/properties/rcsb_ncbi_coordinates/properties/protein_coordinates/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "start_position_on_the_gene_sequence",
              "end_position_on_the_gene_sequence"
            ],
            "properties": {
              "start_position_on_the_gene_sequence": {
                "$id": "#/properties/rcsb_ncbi_coordinates/properties/protein_coordinates/items/properties/start_position_on_the_gene_sequence",
                "type": "integer",
                "title": "The Start_position_on_the_gene_sequence Schema",
                "examples": [
                  1246
                ]
              },
              "end_position_on_the_gene_sequence": {
                "$id": "#/properties/rcsb_ncbi_coordinates/properties/protein_coordinates/items/properties/end_position_on_the_gene_sequence",
                "type": "integer",
                "title": "The End_position_on_the_gene_sequence Schema",
                "examples": [
                  1356
                ]
              }
            }
          }
        }
      }
    },
    "rcsb_ncbi_organism": {
      "$id": "#/properties/rcsb_ncbi_organism",
      "type": "object",
      "title": "The Rcsb_ncbi_organism Schema",
      "required": [
        "tax_id",
        "scientific_name",
        "common_name"
      ],
      "properties": {
        "tax_id": {
          "$id": "#/properties/rcsb_ncbi_organism/properties/tax_id",
          "type": "integer",
          "title": "The Tax_id Schema",
          "examples": [
            9606
          ]
        },
        "scientific_name": {
          "$id": "#/properties/rcsb_ncbi_organism/properties/scientific_name",
          "type": "string",
          "title": "The Scientific_name Schema",
          "examples": [
            "Homo sapiens"
          ],
          "pattern": "^(.*)$"
        },
        "common_name": {
          "$id": "#/properties/rcsb_ncbi_organism/properties/common_name",
          "type": "string",
          "title": "The Common_name Schema",
          "examples": [
            "Human"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}