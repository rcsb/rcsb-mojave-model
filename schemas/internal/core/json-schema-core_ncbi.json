{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "The Root Schema",
  "required": [
    "rcsb_ncbi_containers_identifiers",
    "rcsb_ncbi_metada",
    "rcsb_ncbi_chromosome",
    "rcsb_ncbi_coordinates",
    "rcsb_ncbi_organism"
  ],
  "properties": {
    "rcsb_ncbi_containers_identifiers": {
      "type": "object",
      "description": "The Rcsb_ncbi_containers_identifiers Schema",
      "required": [
        "gene_id",
        "rna_nucleotide_accession",
        "rna_nucleotide_title",
        "rna_nucleotide_version",
        "rna_nucleotide_gi",
        "protein_accession",
        "protein_title",
        "protein_version",
        "protein_gi",
        "symbol",
        "gene_name",
        "uniprot_accession"
      ],
      "properties": {
        "gene_id": {
          "type": "integer",
          "description": "The Gene_id Schema",
          "examples": [
            3265
          ]
        },
        "rna_nucleotide_accession": {
          "type": "string",
          "description": "The Rna_nucleotide_accession Schema",
          "examples": [
            "NM_005343"
          ],
          "pattern": "^(.*)$"
        },
        "rna_nucleotide_title": {
          "type": "string",
          "description": "The Rna_nucleotide_title Schema",
          "examples": [
            "Mus musculus structural maintenance of chromosomes 3 (Smc3), mRNA."
          ],
          "pattern": "^(.*)$"
        },
        "rna_nucleotide_version": {
          "type": "integer",
          "description": "The Rna_nucleotide_version Schema",
          "examples": [
            4
          ]
        },
        "rna_nucleotide_gi": {
          "type": "integer",
          "description": "The Rna_nucleotide_gi Schema",
          "examples": [
            568815587
          ]
        },
        "protein_accession": {
          "type": "string",
          "description": "The Protein_accession Schema",
          "examples": [
            "NP_005334"
          ],
          "pattern": "^(.*)$"
        },
        "protein_title": {
          "type": "string",
          "description": "The Protein_title Schema",
          "examples": [
            "GTPase HRas isoform 1 [Homo sapiens]"
          ],
          "pattern": "^(.*)$"
        },
        "protein_version": {
          "type": "integer",
          "description": "The Protein_version Schema",
          "examples": [
            1
          ]
        },
        "protein_gi": {
          "type": "integer",
          "description": "The Protein_gi Schema",
          "examples": [
            4885425
          ]
        },
        "symbol": {
          "type": "string",
          "description": "The Symbol Schema",
          "examples": [
            "HRAS"
          ],
          "pattern": "^(.*)$"
        },
        "gene_name": {
          "type": "string",
          "description": "The Symbol Schema",
          "examples": [
            "GTPase HRAS human gene"
          ],
          "pattern": "^(.*)$"
        },"uniprot_accession": {
          "type": "array",
          "description": "The Uniprot_accession Schema",
          "items": {
            "type": "object",
            "description": "The Items Schema",
            "required": [
              "accession_id",
              "isoform_id"
            ],
            "properties": {
              "accession_id": {
                "type": "string",
                "description": "The Accession_id Schema",
                "examples": [
                  "P01112"
                ],
                "pattern": "^(.*)$"
              },
              "isoform_id": {
                "type": "integer",
                "description": "The Isoform_id Schema",
                "examples": [
                  1
                ]
              }
            }
          }
        }
      }
    },
    "rcsb_ncbi_metada": {
      "type": "object",
      "description": "The Rcsb_ncbi_metada Schema",
      "required": [
        "status"
      ],
      "properties": {
        "status": {
          "type": "string",
          "description": "The Status Schema",
          "examples": [
            "REVIEWED"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "rcsb_ncbi_chromosome": {
      "type": "object",
      "description": "The Rcsb_ncbi_chromosome Schema",
      "required": [
        "genomic_nucleotide_accession",
        "genomic_nucleotide_version",
        "genomic_nucleotide_gi",
        "assembly",
        "genomic_nucleotide_name",
        "genomic_nucleotide_title"
      ],
      "properties": {
        "genomic_nucleotide_accession": {
          "type": "string",
          "description": "The Genomic_nucleotide_accession Schema",
          "examples": [
            "NC_000011"
          ],
          "pattern": "^(.*)$"
        },
        "genomic_nucleotide_version": {
          "type": "integer",
          "description": "The Genomic_nucleotide_version Schema",
          "examples": [
            10
          ]
        },
        "genomic_nucleotide_gi": {
          "type": "integer",
          "description": "The Genomic_nucleotide_gi Schema",
          "examples": [
            568815587
          ]
        },"assembly": {
          "type": "string",
          "description": "The Genome Assembly Schema",
          "examples": [
            "GRCh38"
          ],
          "pattern": "^(.*)$"
        },
        "genomic_nucleotide_name": {
          "type": "string",
          "description": "The Genomic_nucleotide_name Schema",
          "examples": [
            "11"
          ],
          "pattern": "^(.*)$"
        },
        "genomic_nucleotide_title": {
          "type": "string",
          "description": "The Genomic_nucleotide_title Schema",
          "examples": [
            "Homo sapiens chromosome 11, GRCh38.p12 Primary Assembly"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "rcsb_ncbi_coordinates": {
      "type": "object",
      "description": "The Rcsb_ncbi_coordinates Schema",
      "required": [
        "strat_position_on_the_genomic_accession",
        "end_position_on_the_genomic_accession",
        "orientation",
        "rna_coordinates",
        "protein_coordinates"
      ],
      "properties": {
        "strat_position_on_the_genomic_accession": {
          "type": "integer",
          "description": "The Strat_position_on_the_genomic_accession Schema",
          "examples": [
            532241
          ]
        },
        "end_position_on_the_genomic_accession": {
          "type": "integer",
          "description": "The End_position_on_the_genomic_accession Schema",
          "examples": [
            535566
          ]
        },
        "orientation": {
          "type": "integer",
          "description": "The Orientation Schema",
          "examples": [
            2
          ]
        },
        "rna_coordinates": {
          "type": "array",
          "description": "The Rna_coordinates Schema",
          "items": {
            "type": "object",
            "description": "The Items Schema",
            "required": [
              "start_position_on_the_gene_sequence",
              "end_position_on_the_gene_sequence"
            ],
            "properties": {
              "start_position_on_the_gene_sequence": {
                "type": "integer",
                "description": "The Start_position_on_the_gene_sequence Schema",
                "examples": [
                  1
                ]
              },
              "end_position_on_the_gene_sequence": {
                "type": "integer",
                "description": "The End_position_on_the_gene_sequence Schema",
                "examples": [
                  152
                ]
              }
            }
          }
        },
        "protein_coordinates": {
          "type": "array",
          "description": "The Protein_coordinates Schema",
          "items": {
            "type": "object",
            "description": "The Items Schema",
            "required": [
              "start_position_on_the_gene_sequence",
              "end_position_on_the_gene_sequence"
            ],
            "properties": {
              "start_position_on_the_gene_sequence": {
                "type": "integer",
                "description": "The Start_position_on_the_gene_sequence Schema",
                "examples": [
                  1246
                ]
              },
              "end_position_on_the_gene_sequence": {
                "type": "integer",
                "description": "The End_position_on_the_gene_sequence Schema",
                "examples": [
                  1356
                ]
              }
            }
          }
        }
      }
    },
    "rcsb_ncbi_organism": {
      "type": "object",
      "description": "The Rcsb_ncbi_organism Schema",
      "required": [
        "tax_id",
        "scientific_name",
        "common_name"
      ],
      "properties": {
        "tax_id": {
          "type": "integer",
          "description": "The Tax_id Schema",
          "examples": [
            9606
          ]
        },
        "scientific_name": {
          "type": "string",
          "description": "The Scientific_name Schema",
          "examples": [
            "Homo sapiens"
          ],
          "pattern": "^(.*)$"
        },
        "common_name": {
          "type": "string",
          "description": "The Common_name Schema",
          "examples": [
            "Human"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}