{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title" : "core_ncbi.json",
  "description": "JSON schema for core NCBI collection in RCSB Data Warehouse.\n",
  "required": [
    "rcsb_ncbi_containers_identifiers",
    "rcsb_ncbi_metada",
    "rcsb_ncbi_sequence",
    "rcsb_ncbi_organism"
  ],
  "properties": {
    "rcsb_ncbi_containers_identifiers": {
      "type": "object",
      "description": "RCSB NCBI entry identifiers.\n",
      "required": [
        "protein_accession",
        "protein_title",
        "protein_version",
        "protein_gi",
        "tax_id",
        "uniprot_accession"
      ],
      "properties": {
        "gene_id": {
          "type": "integer",
          "description": "NCBI gene ID.\n",
          "examples": [
            3265
          ]
        },
        "rna_nucleotide_accession": {
          "type": "string",
          "description": "NCBI transcript accession.\n",
          "examples": [
            "NM_005343"
          ],
          "pattern": "^(.*)$"
        },
        "rna_nucleotide_title": {
          "type": "string",
          "description": "NCBI transcript title.\n",
          "examples": [
            "Mus musculus structural maintenance of chromosomes 3 (Smc3), mRNA."
          ],
          "pattern": "^(.*)$"
        },
        "rna_nucleotide_version": {
          "type": "integer",
          "description": "NCBI transcript accession version.\n",
          "examples": [
            4
          ]
        },
        "rna_nucleotide_gi": {
          "type": "integer",
          "description": "NCBI transcript GI number.\n",
          "examples": [
            568815587
          ]
        },
        "protein_accession": {
          "type": "string",
          "description": "NCBI protein accession.\n",
          "examples": [
            "NP_005334"
          ],
          "pattern": "^(.*)$"
        },
        "protein_title": {
          "type": "string",
          "description": "NCBI protein title.\n",
          "examples": [
            "GTPase HRas isoform 1 [Homo sapiens]"
          ],
          "pattern": "^(.*)$"
        },
        "protein_version": {
          "type": "integer",
          "description": "NCBI protein accession title.\n",
          "examples": [
            1
          ]
        },
        "protein_gi": {
          "type": "integer",
          "description": "NCBI protein GI number.\n",
          "examples": [
            4885425
          ]
        },
        "symbol": {
          "type": "string",
          "description": "NCBI gene symbol.\n",
          "examples": [
            "HRAS"
          ],
          "pattern": "^(.*)$"
        },
        "gene_name": {
          "type": "string",
          "description": "NCBI gene name.\n",
          "examples": [
            "GTPase HRAS human gene"
          ],
          "pattern": "^(.*)$"
        },
        "tax_id": {
          "type": "string",
          "description": "NCBI taxonomy ID (String).\n",
          "examples": [
            "NP_005334"
          ],
          "pattern": "^(.*)$"
        },
        "uniprot_accession": {
          "type": "array",
          "description": "UniProt accession list.\n",
          "items": {
            "type": "object",
            "required": [
              "accession_id",
              "isoform_id"
            ],
            "properties": {
              "accession_id": {
                "type": "string",
                "description": "UniProt accesion.\n",
                "examples": [
                  "P01112"
                ],
                "pattern": "^(.*)$"
              },
              "isoform_id": {
                "type": "integer",
                "description": "UniProt isoform ID.\n",
                "examples": [
                  1
                ]
              }
            },
            "additionalProperties": false
          }
        }
      },
      "additionalProperties": false
    },
    "rcsb_ncbi_metada": {
      "type": "object",
      "description": "NCBI metadata.\n",
      "required": [
        "status"
      ],
      "properties": {
        "status": {
          "type": "string",
          "description": "NCBI entry status.\n",
          "examples": [
            "REVIEWED"
          ],
          "pattern": "^(.*)$"
        }
      },
      "additionalProperties": false
    },
    "rcsb_ncbi_chromosome": {
      "type": "object",
      "description": "NCBI entry chromosome information.\n",
      "properties": {
        "genomic_nucleotide_accession": {
          "type": "string",
          "description": "NCBI chromosome accession.\n",
          "examples": [
            "NC_000011"
          ],
          "pattern": "^(.*)$"
        },
        "genomic_nucleotide_version": {
          "type": "integer",
          "description": "NCBI chromosome accession version.\n",
          "examples": [
            10
          ]
        },
        "genomic_nucleotide_gi": {
          "type": "integer",
          "description": "NCBI chromosome GI number.\n",
          "examples": [
            568815587
          ]
        },
        "assembly": {
          "type": "string",
          "description": "Genome assembly name.\n",
          "examples": [
            "GRCh38"
          ],
          "pattern": "^(.*)$"
        },
        "genomic_nucleotide_name": {
          "type": "string",
          "description": "NCBI chromosome name.\n",
          "examples": [
            "11"
          ],
          "pattern": "^(.*)$"
        },
        "genomic_nucleotide_title": {
          "type": "string",
          "description": "NCBI chromosome title.\n",
          "examples": [
            "Homo sapiens chromosome 11, GRCh38.p12 Primary Assembly"
          ],
          "pattern": "^(.*)$"
        }
      },
      "additionalProperties": false
    },
    "rcsb_ncbi_coordinates": {
      "type": "object",
      "description": "NCBI coordinates.\n",
      "properties": {
        "start_position_on_the_genomic_accession": {
          "type": "integer",
          "description": "NCBI gene start position.\n",
          "examples": [
            532241
          ]
        },
        "end_position_on_the_genomic_accession": {
          "type": "integer",
          "description": "NCBI gene end position.\n",
          "examples": [
            535566
          ]
        },
        "orientation": {
          "type": "integer",
          "description": "NCBI gene DNA strand.\n",
          "examples": [
            2
          ]
        },
        "rna_coordinates": {
          "type": "array",
          "description": "NCBI transcript coordinates list.\n",
          "items": {
            "type": "object",
            "required": [
              "start_position_on_the_gene_sequence",
              "end_position_on_the_gene_sequence"
            ],
            "properties": {
              "start_position_on_the_gene_sequence": {
                "type": "integer",
                "description": "NCBI exon start position on the gene.\n",
                "examples": [
                  1
                ]
              },
              "end_position_on_the_gene_sequence": {
                "type": "integer",
                "description": "NCBI exon end position on the gene.\n",
                "examples": [
                  152
                ]
              }
            },
            "additionalProperties": false
          }
        },
        "protein_coordinates": {
          "type": "array",
          "description": "NCBI protein coordinates list.\n",
          "items": {
            "type": "object",
            "required": [
              "start_position_on_the_gene_sequence",
              "end_position_on_the_gene_sequence"
            ],
            "properties": {
              "start_position_on_the_gene_sequence": {
                "type": "integer",
                "description": "NCBI CDS start position on the gene.\n",
                "examples": [
                  1246
                ]
              },
              "end_position_on_the_gene_sequence": {
                "type": "integer",
                "description": "NCBI CDS end position on the gene.\n",
                "examples": [
                  1356
                ]
              }
            },
            "additionalProperties": false
          }
        }
      },
      "additionalProperties": false
    },
    "rcsb_ncbi_organism": {
      "type": "object",
      "description": "NCBI organism information.\n",
      "required": [
        "tax_id",
        "scientific_name"
      ],
      "properties": {
        "tax_id": {
          "type": "integer",
          "description": "NCBI taxonomy ID.\n",
          "examples": [
            9606
          ]
        },
        "scientific_name": {
          "type": "string",
          "description": "NCBI organism scientific name.\n",
          "examples": [
            "Homo sapiens"
          ],
          "pattern": "^(.*)$"
        },
        "common_name": {
          "type": "string",
          "description": "NCBI organism common name.\n",
          "examples": [
            "Human"
          ],
          "pattern": "^(.*)$"
        }
      },
      "additionalProperties": false
    },
    "rcsb_ncbi_alignments": {
      "type": "object",
      "description": "NCBI pairwise sequence alignments.\n",
      "properties": {
        "core_entity_alignments": {
          "type": "array",
          "description": "List of alignments with core_entity canonical sequences",
          "items": {
            "type": "object",
            "description": "Alignment with a core_entity canonical sequence",
            "required": ["rcsb_entity_container_identifiers","scores","aligned_regions"],
            "properties": {
              "rcsb_entity_container_identifiers": {
                "type": "object",
                "description": "core_entity identifiers",
                "required": ["entity_id","entry_id"],
                "properties": {
                  "entry_id": {
                    "type": "string"
                  },
                  "entity_id": {
                    "type": "integer"
                  }
                }
              },
              "scores": {
                "type": "object",
                "description": "Alignment scores",
                "required": ["target_coverage","query_coverage"],
                "properties": {
                  "target_coverage": {
                    "type": "integer"
                  },
                  "query_coverage": {
                    "type": "integer"
                  }
                }
              },
              "aligned_regions": {
                "type": "array",
                "description": "Aligned sequence regions",
                "items": {
                  "type": "object",
                  "description": "Aligned region",
                  "required": ["target_begin","query_begin","length"],
                  "properties": {
                    "target_begin":{
                      "type": "integer",
                      "description": "NCBI sequence start position"
                    },
                    "query_begin": {
                      "type": "integer",
                      "description": "Entity seqeunce start position"
                    },
                    "length": {
                      "type": "integer",
                      "description": "Aligned region length"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "additionalProperties": false
    },
    "rcsb_ncbi_sequence": {
      "type": "object",
      "description": "NCBI sequences.\n",
      "required": [
        "protein"
      ],
      "properties": {
        "protein": {
          "type": "string",
          "description": "NCBI protein sequence.\n",
          "examples": [
            "MTEYKLVVVGAGGVGKSALTIQLIQNHFVDEYDPTIEDSYRKQVVIDGETCLLDILDTAGQEEYSAMRDQYMRTGEGFLCVFAINNTKSFEDIHQYREQIKRVKDSDDVPMVLVGNKCDLAARTVESRQAQDLARSYGIPYIETSAKTRQGVEDAFYTLVREIRQHKLRKLNPPDESGPGCMSCKCVLS"
          ]
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
