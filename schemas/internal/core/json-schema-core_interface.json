{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "title" : "Core Assembly",
  "description": "JSON schema for entity level core data. Combines RCSB Exchange Database schema: json-schema-min-pdbx_core_assembly.json and RCSB Data Warehouse schema: json-schema-core_assembly.json",
  "type": "object",
  "required": [
    ""
  ],
  "properties": {
    "rcsb_id": {
      "type": "string"
    },
    "rcsb_interface_container_identifier": {
      "type": "object",
      "properties": {
        "entry_id": {
          "type": "string",
          "description": "Entry identifier for the container."
        },
        "assembly_id": {
          "type": "string",
          "description": "This item references an assembly in pdbx_struct_assembly"
        },
        "interface_id": {
          "type": "string",
          "description": "Identifier for the equivalent interfaces within the assembly"
        },
        "rcsb_id": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "required": [
        "entry_id",
        "assembly_id"
      ]
    },
    "rcsb_interface_partner_1": {
      "type": "object",
      "properties": {
        "rcsb_interface_partner_container_identifier": {
          "type": "object",
          "properties": {
            "entity_id": {
              "type": "string",
              "description": "Polymer entity identifier for the container."
            },
            "asym_id": {
              "type": "string",
              "description": "Instance identifier for this container."
            }
          },
          "additionalProperties": false,
          "required": [
            "asym_id",
            "entity_id"
          ]
        },
        "rcsb_interface_partner_feature": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "assignment_version": {
                "type": "string",
                "examples": [
                  "V4_0_2"
                ],
                "description": "Identifies the version of the feature assignment."
              },
              "description": {
                "type": "string",
                "description": "A description for the feature."
              },
              "feature_id": {
                "type": "string",
                "description": "An identifier for the feature."
              },
              "name": {
                "type": "string",
                "description": "A name for the feature."
              },
              "provenance_source": {
                "type": "string",
                "examples": [
                  "NACCESS"
                ],
                "description": "Code identifying the individual, organization or program that\n assigned the feature."
              },
              "type": {
                "type": "string",
                "enum": [
                  "ASA_UNBOUND",
                  "ASA_BOUND",
                  "BINDING_SITE"
                ],
                "examples": [
                  "ASA_UNBOUND",
                  "BINDING_SITE"
                ],
                "description": "A type or category of the feature."
              },
              "feature_positions": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "beg_seq_id": {
                      "type": "integer",
                      "description": "An identifier for the monomer at which this segment of the feature begins."
                    },
                    "end_seq_id": {
                      "type": "integer",
                      "description": "An identifier for the monomer at which this segment of the feature ends."
                    },
                    "values": {
                      "type": "array",
                      "items": {
                        "type": "number",
                        "examples": [
                          1.5,
                          0.25
                        ],
                        "description": "The value(s) of the feature over the monomer segment."
                      },
                      "uniqueItems": false
                    }
                  },
                  "additionalProperties": false,
                  "required": [
                    "beg_seq_id"
                  ]
                },
                "uniqueItems": false
              },
              "additional_properties": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "enum": [
                        "TO_BE_DEFINED"
                      ],
                      "examples": [
                        "TO_BE_DEFINED"
                      ],
                      "description": "The additional property name."
                    },
                    "values": {
                      "type": "array",
                      "items": {
                        "anyOf": [
                          {
                            "type": "string"
                          },
                          {
                            "type": "integer"
                          },
                          {
                            "type": "number"
                          }
                        ],
                        "examples": [
                          "globin-like",
                          "2.54",
                          "300"
                        ],
                        "description": "The value(s) of the additional property."
                      },
                      "uniqueItems": false
                    }
                  },
                  "additionalProperties": false
                },
                "uniqueItems": false
              }
            },
            "additionalProperties": false,
            "required": [
              "ordinal"
            ]
          },
          "minItems": 1,
          "uniqueItems": true
        }
      }
    },
    "rcsb_interface_partner_2": {
      "$ref": "#/properties/rcsb_interface_partner_1"
    },
    "rcsb_interface_operator_pair": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "rcsb_interface_operator_partner_1": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Identifies the list of operations from the category pdbx_struct_oper_list. One item in array per operator applied. The order follows how operators are applied."
          },
          "rcsbinterface_operator_partner_2": {
            "$ref": "#/properties/rcsb_interface_operator_pair/items/properties/rcsb_interface_operator_partner_1"
          }
        }
      }
    },
    "rcsb_interface_property": {
      "type": "object",
      "description": "Global interface properties (e.g. number of interface residues, interface accessibility, etc.)",
      "properties": {
        "assignment_version": {
          "type": "string",
          "examples": [
            "V4_0_2"
          ],
          "description": "Identifies the version of the feature assignment."
        },
        "description": {
          "type": "string",
          "description": "A description for the feature."
        },
        "property_id": {
          "type": "string",
          "description": "An identifier for the feature."
        },
        "name": {
          "type": "string",
          "description": "A name for the feature."
        },
        "provenance_source": {
          "type": "string",
          "examples": [
            "NACCESS"
          ],
          "description": "Code identifying the individual, organization or program that\n assigned the feature."
        },
        "type": {
          "type": "string",
          "enum": [
            "TOTAL_ASA",
            "RESIDUE_NUMBER",
            "HOMO_IOU"
          ],
          "examples": [
            "TOTAL_ASA",
            "RESIDUE_NUMBER",
            "HOMO_IOU"
          ],
          "description": "A type or category of the feature."
        },
        "value": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            },
            {
              "type": "number"
            }
          ],
          "examples": [
            "hetero",
            "2.54",
            "300"
          ],
          "description": "The value of the property."
        }
      }
    }
  },
  "additionalProperties": false
}
