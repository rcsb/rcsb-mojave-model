{
  "type" : "object",
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "title" : "core_uniprot.json",
  "required" : [
    "rcsb_uniprot_container_identifiers"
  ],
  "properties" : {
    "rcsb_uniprot_container_identifiers" : {
      "type" : "object",
      "properties" : {
        "uniprot_id" : {
          "type" : "string",
          "description" : "Primary accession number of a given UniProtKB entry.\n"
        },
        "pfam_ids" : {
          "type" : "array",
          "items" : {
            "type" : "string",
            "description" : "Unique Pfam identifiers mapped to a given UniProtKB entry.\n"
          },
          "uniqueItems" : true
        },
        "go_ids" : {
          "type" : "array",
          "items" : {
            "type" : "string",
            "description" : "Unique GO identifiers mapped to a given UniProtKB entry.\n"
          },
          "uniqueItems" : true
        },
        "ensembl_ids" : {
          "type" : "array",
          "items" : {
            "type" : "string",
            "description" : "Unique Ensembl identifiers mapped to a given UniProtKB entry.\n"
          },
          "uniqueItems" : true
        }
      },
      "additionalProperties": false
    },
    "accession" : {
      "type" : "array",
      "items" : {
        "type" : "string"
      },
      "description": "List of UniProtKB accession numbers where original accession numbers are retained as ‘secondary’ accession numbers.\n"
    },
    "sequence" : {
      "type" : "object",
      "properties" : {
        "length" : {
          "type" : "integer"
        },
        "value" : {
          "type" : "string"
        },
        "version" : {
          "type" : "integer"
        }
      },
      "additionalProperties": false,
      "description": "Protein sequence data for canonical protein sequence.\n"
    },
    "name" : {
      "type" : "array",
      "items" : {
        "type" : "string"
      },
      "description" : "A list of unique identifiers (former IDs), often containing biologically relevant information.\n"
    },
    "keyword" : {
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "string",
            "examples": ["KW-0275", "KW-0597"],
            "description": "A unique keyword identifier.\n"
          },
          "value" : {
            "type" : "string",
            "examples": [ "Lipid metabolism", "Phosphoprotein", "Fatty acid biosynthesis" ],
            "description": "Human-readable keyword term."
          }
        },
        "additionalProperties": false
      },
      "description": "Keywords constitute a controlled vocabulary that summarises the content of a UniProtKB entry.\n"
    },
    "gene" : {
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "name" : {
            "type" : "array",
            "items" : {
              "type" : "object",
              "properties" : {
                "type" : {
                  "enum" : [
                    "PRIMARY",
                    "SYNONYM",
                    "ORDERED_LOCUS",
                    "ORF"
                  ],
                  "type" : "string"
                },
                "value" : {
                  "type" : "string"
                }
              }
            }
          }
        },
        "additionalProperties": false,
        "description" : "The name(s) of the gene(s) that code for the protein sequence(s) described in the entry.\n"
      }
    },
    "protein" : {
      "type" : "object",
      "properties" : {
        "component" : {
          "type" : "array",
          "items" : {
            "type" : "object",
            "properties" : {
              "alternative_name" : {
                "type" : "array",
                "items" : {
                  "type" : "object",
                  "properties" : {
                    "ec_number" : {
                      "type" : "array",
                      "items" : {
                        "type" : "object",
                        "properties" : {
                          "value" : {
                            "type" : "string"
                          }
                        },
                        "additionalProperties": false
                      }
                    },
                    "full_name" : {
                      "type" : "object",
                      "properties" : {
                        "value" : {
                          "type" : "string"
                        }
                      },
                      "additionalProperties": false
                    },
                    "short_name" : {
                      "type" : "array",
                      "items" : {
                        "type" : "object",
                        "properties" : {
                          "value" : {
                            "type" : "string"
                          }
                        },
                        "additionalProperties": false
                      }
                    }
                  }
                }
              },
              "recommended_name" : {
                "type" : "object",
                "properties" : {
                  "ec_number" : {
                    "type" : "array",
                    "items" : {
                      "type" : "object",
                      "properties" : {
                        "value" : {
                          "type" : "string"
                        }
                      },
                      "additionalProperties": false
                    }
                  },
                  "full_name" : {
                    "type" : "object",
                    "properties" : {
                      "value" : {
                        "type" : "string"
                      }
                    },
                    "additionalProperties": false
                  },
                  "short_name" : {
                    "type" : "array",
                    "items" : {
                      "type" : "object",
                      "properties" : {
                        "value" : {
                          "type" : "string"
                        }
                      },
                      "additionalProperties": false
                    }
                  }
                }
              },
              "submitted_name" : {
                "type" : "array",
                "items" : {
                  "type" : "object",
                  "properties" : {
                    "ec_number" : {
                      "type" : "array",
                      "items" : {
                        "type" : "object",
                        "properties" : {
                          "value" : {
                            "type" : "string"
                          }
                        }
                      }
                    },
                    "full_name" : {
                      "type" : "object",
                      "properties" : {
                        "value" : {
                          "type" : "string"
                        }
                      },
                      "additionalProperties": false
                    }
                  },
                  "additionalProperties": false
                }
              }
            },
            "additionalProperties": false
          }
        },
        "domain" : {
          "type" : "array",
          "items" : {
            "type" : "object",
            "properties" : {
              "alternative_name" : {
                "type" : "array",
                "items" : {
                  "type" : "object",
                  "properties" : {
                    "ec_number" : {
                      "type" : "array",
                      "items" : {
                        "type" : "object",
                        "properties" : {
                          "value" : {
                            "type" : "string"
                          }
                        }
                      }
                    },
                    "full_name" : {
                      "type" : "object",
                      "properties" : {
                        "value" : {
                          "type" : "string"
                        }
                      }
                    },
                    "short_name" : {
                      "type" : "array",
                      "items" : {
                        "type" : "object",
                        "properties" : {
                          "value" : {
                            "type" : "string"
                          }
                        }
                      }
                    }
                  }
                }
              },
              "recommended_name" : {
                "type" : "object",
                "properties" : {
                  "ec_number" : {
                    "type" : "array",
                    "items" : {
                      "type" : "object",
                      "properties" : {
                        "value" : {
                          "type" : "string"
                        }
                      }
                    }
                  },
                  "full_name" : {
                    "type" : "object",
                    "properties" : {
                      "value" : {
                        "type" : "string"
                      }
                    }
                  },
                  "short_name" : {
                    "type" : "array",
                    "items" : {
                      "type" : "object",
                      "properties" : {
                        "value" : {
                          "type" : "string"
                        }
                      }
                    }
                  }
                }
              },
              "submitted_name" : {
                "type" : "array",
                "items" : {
                  "type" : "object",
                  "properties" : {
                    "ec_number" : {
                      "type" : "array",
                      "items" : {
                        "type" : "object",
                        "properties" : {
                          "value" : {
                            "type" : "string"
                          }
                        }
                      }
                    },
                    "full_name" : {
                      "type" : "object",
                      "properties" : {
                        "value" : {
                          "type" : "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "alternative_name" : {
          "type" : "array",
          "items" : {
            "type" : "object",
            "properties" : {
              "ec_number" : {
                "type" : "array",
                "items" : {
                  "type" : "object",
                  "properties" : {
                    "value" : {
                      "type" : "string",
                      "description": "Enzyme Commission number.\n"
                    }
                  },
                  "additionalProperties": false
                }
              },
              "full_name" : {
                "type" : "object",
                "properties" : {
                  "value" : {
                    "type" : "string",
                    "description": "Synonym of the recommended name (full name).\n"
                  }
                },
                "additionalProperties": false
              },
              "short_name" : {
                "type" : "array",
                "items" : {
                  "type" : "object",
                  "properties" : {
                    "value" : {
                      "type" : "string",
                      "description": "Abbreviation of the full name or an acronym.\n"
                    }
                  },
                  "additionalProperties": false
                }
              }
            },
            "additionalProperties": false
          },
          "description": "Synonyms of the recommended name (full name).\n"
        },
        "recommended_name" : {
          "type" : "object",
          "properties" : {
            "ec_number" : {
              "type" : "array",
              "items" : {
                "type" : "object",
                "properties" : {
                  "value" : {
                    "type" : "string",
                    "description": "Enzyme Commission number.\n"
                  }
                },
                "additionalProperties": false
              }
            },
            "full_name" : {
              "type" : "object",
              "properties" : {
                "value" : {
                  "type" : "string",
                  "description": "Full name recommended by the UniProt consortium.\n"
                }
              },
              "additionalProperties": false
            },
            "short_name" : {
              "type" : "array",
              "items" : {
                "type" : "object",
                "properties" : {
                  "value" : {
                    "type" : "string",
                    "description": "Abbreviation of the full name or acronym.\n"
                  }
                },
                "additionalProperties": false
              }
            }
          },
          "additionalProperties": false,
          "description": "Name recommended by the UniProt consortium.\n"
        },
        "submitted_name" : {
          "type" : "array",
          "items" : {
            "type" : "object",
            "properties" : {
              "ec_number" : {
                "type" : "array",
                "items" : {
                  "type" : "object",
                  "properties" : {
                    "value" : {
                      "type" : "string"
                    }
                  }
                }
              },
              "full_name" : {
                "type" : "object",
                "properties" : {
                  "value" : {
                    "type" : "string"
                  }
                }
              }
            },
            "additionalProperties": false
          },
          "description": "UniProtKB/TrEMBL entries usually have Submitted name instead of Recommended name.\n Submitted names may later be improved by automatic annotation procedures.\n"
        }
      },
      "additionalProperties": false,
      "description": "An exhaustive list of all names of the protein, from commonly used to obsolete, to allow unambiguous identification of a protein.\n"
    },
    "organism" : {
      "type" : "object",
      "properties" : {
        "lineage" : {
          "type" : "object",
          "properties" : {
            "taxon" : {
              "type" : "array",
              "items" : {
                "type" : "string"
              }
            }
          }
        },
        "name" : {
          "type" : "array",
          "items" : {
            "type" : "object",
            "properties" : {
              "type" : {
                "enum" : [
                  "COMMON",
                  "FULL",
                  "SCIENTIFIC",
                  "SYNONYM",
                  "ABBREVIATION"
                ],
                "type" : "string"
              },
              "value" : {
                "type" : "string"
              }
            }
          },
          "description": "The organism designation consists of the Latin scientific name,\n usually composed of the genus and species names (the binomial system developed by Linnaeus),\n the English common name, a synonym, etc.\n"
        }
      },
      "additionalProperties" : false,
      "description": "Information on the name(s) of the organism that is the source of the protein sequence.\n"
    },
    "rcsb_protein_name": {
      "type" : "object",
      "required" : [ "value", "provenance_code" ],
      "properties" : {
        "value" : {
          "type" : "string",
          "description" : "Name that allows to unambiguously identify a protein.\n"
        },
        "provenance_code" : {
          "type" : "string",
          "description" : "Historical record of the data attribute.\n"
        }
      },
      "additionalProperties": false
    },
    "rcsb_ec" : {
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "number" : {
            "type" : "string"
          },
          "provenance_code" : {
            "type" : "string",
            "description" : "Historical record of the data attribute.\n"
          }
        },
        "additionalProperties": false,
        "description" : "Enzyme Commission (EC) number(s).\n"
      },
      "minItems" : 1,
      "uniqueItems" : true
    },
    "rcsb_organism" : {
      "type" : "object",
      "required" : [ "scientific_name", "provenance_code" ],
      "properties" : {
        "scientific_name" : {
          "type" : "string",
          "description" : "The scientific name of the organism in which a protein occurs.\n"
        },
        "taxonomy_id" : {
          "type" : "integer",
          "description" : "NCBI Taxonomy identifier for the organism in which a protein occurs.\n"
        },
        "provenance_code" : {
          "type" : "string",
          "description" : "Historical record of the data attribute.\n"
        }
      },
      "additionalProperties": false,
      "description": "Taxonomy information on the organism that is the source of the protein sequence.\n"
    },
    "rcsb_protein_function" : {
      "type" : "object",
      "required" : [ "details", "provenance_code" ],
      "properties" : {
        "details" : {
          "type" : "string",
          "description" : "General function(s) of a protein.\n"
        },
        "provenance_code" : {
          "type" : "string",
          "description" : "Historical record of the data attribute.\n"
        }
      },
      "additionalProperties": false
    },
    "rcsb_protein_feature" : {
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "string",
            "description": "Feature identifier, which allows to construct links directly from position-specific annotation to specialized protein-related databases.\n"
          },
          "type" : {
            "enum" : [
              "ACTIVE_SITE",
              "BINDING_SITE",
              "CALCIUM_BINDING_REGION",
              "CHAIN",
              "COILED_COIL_REGION",
              "COMPOSITIONALLY_BIASED_REGION",
              "CROSS_LINK",
              "DISULFIDE_BOND",
              "DNA_BINDING_REGION",
              "DOMAIN",
              "GLYCOSYLATION_SITE",
              "HELIX",
              "INITIATOR_METHIONINE",
              "LIPID_MOIETY_BINDING_REGION",
              "METAL_ION_BINDING_SITE",
              "MODIFIED_RESIDUE",
              "MUTAGENESIS_SITE",
              "NON_CONSECUTIVE_RESIDUES",
              "NON_TERMINAL_RESIDUE",
              "NUCLEOTIDE_PHOSPHATE_BINDING_REGION",
              "PEPTIDE",
              "PROPEPTIDE",
              "REGION_OF_INTEREST",
              "REPEAT",
              "NON_STANDARD_AMINO_ACID",
              "SEQUENCE_CONFLICT",
              "SEQUENCE_VARIANT",
              "SHORT_SEQUENCE_MOTIF",
              "SIGNAL_PEPTIDE",
              "SITE",
              "SPLICE_VARIANT",
              "STRAND",
              "TOPOLOGICAL_DOMAIN",
              "TRANSIT_PEPTIDE",
              "TRANSMEMBRANE_REGION",
              "TURN",
              "UNSURE_RESIDUE",
              "ZINC_FINGER_REGION",
              "INTRAMEMBRANE_REGION"
            ],
            "type" : "string",
            "description": "Topics for which feature identifiers currently exist.\n"
          },
          "location" : {
            "type" : "object",
            "properties" : {
              "begin" : {
                "type" : "integer"
              },
              "end" : {
                "type" : "integer"
              },
              "position" : {
                "type" : "integer"
              }
            },
            "additionalProperties": false
          },
          "description" : {
            "type" : "string",
            "description": "Human-readable feature description.\n"
          }
        },
        "additionalProperties": false
      },
      "description": "Sequence annotations describe regions or sites of interest in the protein sequence,\n such as post-translational modifications, binding sites, enzyme active sites, local secondary structure \nor other characteristics reported in the cited references.\n"
    }
  },
  "additionalProperties" : false
}
