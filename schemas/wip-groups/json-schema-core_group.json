{
  "type": "object",
  "required": [
    "rcsb_id",
    "rcsb_group_container_identifiers",
    "rcsb_group_info",
    "rcsb_group_statistics"
  ],
  "properties": {
    "rcsb_id": {
      "type": "string",
      "description": "A unique textual identifier for a group"
    },
    "rcsb_group_container_identifiers": {
      "type": "object",
      "required": [
        "group_id",
        "group_provenance_id",
        "group_members"
      ],
      "properties": {
        "group_id": {
          "type": "string",
          "description": "A unique textual identifier for a group"
        },
        "group_provenance_id": {
          "type": {
            "type": "string",
            "description": "This data item is a pointer to a group provenance definition"
          }
        },
        "parent_members": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "parent_id": {
                "type": "string"
              }
            }
          },
          "minItems": 1,
          "uniqueItems": true,
          "description": "Member identifiers representing a higher level in the groping hierarchy that has parent-child relationship"
        },
        "group_members": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "member_id"
            ],
            "properties": {
              "member_id": {
                "type": "string"
              },
              "ranking": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "ranking_criteria_id": {
                      "type": "string"
                    },
                    "representative_criteria_id": {
                      "type": "string"
                    },
                    "ranking_score": {
                      "type": "number",
                      "description": "Quantifies the criteria used for ranking"
                    }
                  }
                }
              },
              "additionalProperties": false,
              "description": "A list of members belonging to a group"
            },
            "minItems": 1,
            "uniqueItems": true
          }
        }
      },
      "additionalProperties": false
    },
    "rcsb_group_info": {
      "required": [
        "group_name",
        "group_description",
        "group_members_granularity",
        "group_members_count"
      ],
      "group_name": {
        "type": "string"
      },
      "group_description": {
        "type": "string"
      },
      "group_members_granularity": {
        "type": "string",
        "enum": [
          "assembly",
          "polymer_entity",
          "polymer_entity_instance"
        ],
        "description": "Granularity of group members"
      },
      "group_members_count": {
        "type": "integer"
      }
    },
    "rcsb_group_statistics": {
      "type": "object",
      "properties": {
        "similarity_score_min": {
          "type": "number",
          "description": ""
        },
        "similarity_score_max": {
          "type": "number",
          "description": ""
        }
      },
      "additionalProperties": false
    },
    "rcsb_group_accession_info": {
      "type": "object",
      "required": [
        "version"
      ],
      "properties": {
        "version": {
          "type": "integer",
          "description": ""
        }
      },
      "additionalProperties": false
    },
    "rcsb_group_aggregation_details": {
      "type": "object",
      "properties": {
        "group_reference": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "group_reference_id": {
                "type": "string",
                "description": "A unique label assigned to a reference that identifies the group"
              },
              "type": {
                "type": "string",
                "examples": [
                  "CATH number",
                  "UniProt accession"
                ],
                "description": ""
              }
            }
          },
          "minItems": 1,
          "uniqueItems": true,
          "description": "Identifiers referring to a higher level in the members groping hierarchy"
        }
      }
    },
    "rcsb_group_representatives": {
      "type": "array",
      "items": {
        "representative_criteria_id": {
          "type": "string"
        },
        "member_id": {
          "type": "string"
        }
      },
      "description": "Representatives of the group (e.g. a member that minimizes the sum distances to all members)"
    }
  },
  "additionalProperties": false
}
